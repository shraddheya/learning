{
  "name": "testingsqlitesecure",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:build:watch": "ng build --watch",
    "ng:build": "ng build --prod",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "ng e2e",
    "el:start": "electron . --serve",
    "el:start:wait": "wait-on http-get://localhost:4200/ && electronmon . --serve",
    "el:start:wait:prod": "electronmon .",
    "test": "npm-run-all -p ng:start el:start:wait",
    "test:prod": "npm-run-all -p ng:build el:start:wait:prod",
    "rebuild:sqlite": "electron-rebuild -f -w sqlite3",
    "build:linux": "npm run ng:build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "build:windows": "npm run ng:build && electron-packager . testingsqlitesecure --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"HRMS_EIS\"",
    "build:mac": "npm run ng:build && electron-packager . testingsqlitesecure --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "ngx-electron": "^2.1.1",
    "rxjs": "~6.4.0",
    "sequelize": "^5.18.1",
    "sqlite3": "^4.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron": "^6.0.7",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.5",
    "electron-rebuild": "^1.8.6",
    "electron-reload": "^1.5.0",
    "electronmon": "^0.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "^3.3.0"
  },
  "main": "electronMain.js",
  "author": {
    "name": "Shraddheya Shrivastava",
    "website": "https://www.shraddheya.com"
  }
}
